; 5384 : // read 16bit value and convert to 24bit RGB
; 5385 : static void stbi__tga_read_rgb16(stbi__context* s, stbi_uc* out)
; 5386 : {
; 5387 :     stbi__uint16 px = (stbi__uint16)stbi__get16le(s);
